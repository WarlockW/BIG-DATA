<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="child" class="SpringFramework_parent.Person" depends-on="father" parent="father">
        <property name="name" value="child"></property>
        <property name="age" value="18"></property>
    </bean>

    <bean id="father" class="SpringFramework_parent.Person">
        <property name="name" value="father"></property>
        <property name="age" value="50"></property>
        <property name="address" value="北京市海淀区"></property>
    </bean>
</beans>